{
  "name": "browser-emulator",
  "version": "2.2.0",
  "description": "Load testing tool for OpenVidu",
  "main": "server.ts",
  "scripts": {
    "// FIXME Canvas forces importing the DOM module:": "",
    "// https://github.com/Automattic/node-canvas/issues/1656": "",
    "//postinstall": "rm -f ./node_modules/typescript/lib/lib.dom.d.ts",
    "start:prod": "EMULATED_USER_TYPE=NONE node ./dist/server.js",
    "start:prod-canvas": "EMULATED_USER_TYPE=NODE_WEBRTC_CANVAS node ./dist/server.js",
    "start:prod-kms": "EMULATED_USER_TYPE=KMS node ./dist/server.js",
    "start:dev-kms": "APPLICATION_MODE=DEVELOPMENT EMULATED_USER_TYPE=KMS ts-node ./src/server.ts",
    "start:dev-canvas": "APPLICATION_MODE=DEVELOPMENT EMULATED_USER_TYPE=NODE_WEBRTC_CANVAS ts-node ./src/server.ts",
    "start:dev-none": "APPLICATION_MODE=DEVELOPMENT EMULATED_USER_TYPE=NONE ts-node ./dist/server.js",
    "start:docker": "docker run --name browser-emulator -d -it --rm --network browseremulator -p 5000:5000 -p 5001:5001 --shm-size=2.5gb -v /var/run/docker.sock:/var/run/docker.sock -v /opt/openvidu-loadtest/browser-emulator/selenoid-config:/opt/openvidu-loadtest/browser-emulator/selenoid-config -v /opt/openvidu-loadtest/browser-emulator/recordings:/opt/openvidu-loadtest/browser-emulator/recordings -v /opt/openvidu-loadtest/browser-emulator/src/assets/mediafiles:/opt/openvidu-loadtest/browser-emulator/src/assets/mediafiles openvidu/browser-emulator:2.2.0",
    "build": "tsc",
    "test": "echo 'Error: no test specified' && exit 1",
    "qoe": "ts-node ./src/utils/run-qoe-analysis.ts"
  },
  "author": "Carlos Santos",
  "license": "Apache 2.0",
  "dependencies": {
    "@elastic/elasticsearch": "7.14.1",
    "aws-sdk": "^2.1148.0",
    "axios": "0.23.0",
    "btoa": "1.2.1",
    "canvas": "2.10.2",
    "dockerode": "3.3.1",
    "express": "4.17.1",
    "fluent-ffmpeg": "2.1.2",
    "fluent-ffmpeg-multistream": "1.0.0",
    "kurento-client": "6.16.0",
    "minio": "^7.0.32",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^2.6.7",
    "node-localstorage": "2.2.1",
    "node-os-utils": "1.3.5",
    "openvidu-browser": "^2.23.0",
    "p-limit": "^3.1.0",
    "platform": "1.3.6",
    "selenium-webdriver": "4.6.1",
    "stream-chunker": "1.2.8",
    "wrtc": "0.4.7",
    "ws": "^8.7.0",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@tsconfig/node14": "1.0.1",
    "@types/dockerode": "3.2.3",
    "@types/express": "4.17.12",
    "@types/minio": "^7.0.14",
    "@types/multer": "^1.4.7",
    "@types/node": "14.14.31",
    "@types/node-fetch": "2.6.1",
    "@types/node-os-utils": "1.2.0",
    "@types/platform": "1.3.3",
    "@types/selenium-webdriver": "4.0.18",
    "@types/ws": "7.4.1",
    "@types/yargs": "^17.0.10",
    "prettier": "2.2.0",
    "ts-node": "9.1.1",
    "typescript": "4.2.4"
  }
}
