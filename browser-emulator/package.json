{
  "name": "browser-emulator",
  "version": "2.1.0",
  "description": "Load testing tool for OpenVidu",
  "main": "server.ts",
  "scripts": {
    "// FIXME Canvas forces importing the DOM module:": "",
    "// https://github.com/Automattic/node-canvas/issues/1656": "",
    "//postinstall": "rm -f ./node_modules/typescript/lib/lib.dom.d.ts",
    "start:prod": "EMULATED_USER_TYPE=KMS nodemon ./dist/server.js",
    "start:dev-kms": "APPLICATION_MODE=DEVELOPMENT EMULATED_USER_TYPE=KMS nodemon ./src/server.ts",
    "start:dev-canvas": "APPLICATION_MODE=DEVELOPMENT EMULATED_USER_TYPE=NODE_WEBRTC_CANVAS nodemon ./src/server.ts",
    "build": "tsc",
    "test": "echo 'Error: no test specified' && exit 1"
  },
  "author": "Carlos Santos",
  "license": "Apache 2.0",
  "dependencies": {
    "@elastic/elasticsearch": "7.14.1",
    "aws-sdk": "2.1006.0",
    "axios": "0.23.0",
    "btoa": "1.2.1",
    "canvas": "2.8.0",
    "dockerode": "3.3.1",
    "express": "4.17.1",
    "fluent-ffmpeg": "2.1.2",
    "fluent-ffmpeg-multistream": "1.0.0",
    "kurento-client": "6.16.0",
    "node-fetch": "3.1.1",
    "node-localstorage": "2.2.1",
    "node-os-utils": "1.3.5",
    "nodemon": "2.0.13",
    "openvidu-browser": "2.20.0",
    "platform": "1.3.6",
    "selenium-webdriver": "4.0.0",
    "stream-chunker": "1.2.8",
    "wrtc": "0.4.7",
    "ws": "7.4.6"
  },
  "devDependencies": {
    "@tsconfig/node14": "1.0.1",
    "@types/dockerode": "3.2.3",
    "@types/express": "4.17.12",
    "@types/node": "14.14.31",
    "@types/node-fetch": "2.5.8",
    "@types/node-os-utils": "1.2.0",
    "@types/platform": "1.3.3",
    "@types/selenium-webdriver": "4.0.12",
    "@types/ws": "7.4.1",
    "prettier": "2.2.0",
    "ts-node": "9.1.1",
    "typescript": "4.2.4"
  }
}
