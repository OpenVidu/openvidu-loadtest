{
  "name": "browser-emulator",
  "version": "2.2.0",
  "description": "Load testing tool for OpenVidu",
  "main": "server.ts",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "// FIXME Canvas forces importing the DOM module:": "",
    "// https://github.com/Automattic/node-canvas/issues/1656": "",
    "//postinstall": "rm -f ./node_modules/typescript/lib/lib.dom.d.ts",
    "start:prod": "EMULATED_USER_TYPE=NONE node ./dist/server.js",
    "start:prod-livekit": "EMULATED_USER_TYPE=NONE COM_MODULE=livekit node ./dist/server.js",
    "start:prod-firefox": "EMULATED_USER_TYPE=NONE REAL_DRIVER=firefox node ./dist/server.js",
    "start:prod-livekit-firefox": "EMULATED_USER_TYPE=NONE REAL_DRIVER=firefox COM_MODULE=livekit node ./dist/server.js",
    "start:prod-canvas": "EMULATED_USER_TYPE=NODE_WEBRTC_CANVAS node ./dist/server.js",
    "start:prod-kms": "EMULATED_USER_TYPE=KMS node ./dist/server.js",
    "start:dev-kms": "APPLICATION_MODE=DEVELOPMENT EMULATED_USER_TYPE=KMS ts-node ./src/server.ts",
    "start:dev-canvas": "APPLICATION_MODE=DEVELOPMENT EMULATED_USER_TYPE=NODE_WEBRTC_CANVAS ts-node ./src/server.ts",
    "start:dev-none": "APPLICATION_MODE=DEVELOPMENT EMULATED_USER_TYPE=NONE ts-node ./src/server.ts",
    "start:dev-none-firefox": "APPLICATION_MODE=DEVELOPMENT EMULATED_USER_TYPE=NONE REAL_DRIVER=firefox ts-node ./src/server.ts",
    "start:dev-livekit-firefox": "APPLICATION_MODE=DEVELOPMENT EMULATED_USER_TYPE=NONE REAL_DRIVER=firefox COM_MODULE=livekit ts-node ./src/server.ts",
    "start:docker": "docker run --name browser-emulator -d -it --rm --network browseremulator -p 5000:5000 -p 5001:5001 --shm-size=2.5gb -v /var/run/docker.sock:/var/run/docker.sock -v /opt/openvidu-loadtest/browser-emulator/recordings:/opt/openvidu-loadtest/browser-emulator/recordings -v /opt/openvidu-loadtest/browser-emulator/src/assets/mediafiles:/opt/openvidu-loadtest/browser-emulator/src/assets/mediafiles openvidu/browser-emulator:2.2.0",
    "build": "tsc",
    "watch": "tsc -w",
    "test": "echo 'Error: no test specified' && exit 1",
    "qoe": "ts-node ./src/utils/run-qoe-analysis.ts"
  },
  "author": "Carlos Santos",
  "license": "Apache 2.0",
  "dependencies": {
    "@elastic/elasticsearch": "7.8.0",
    "aws-sdk": "^2.1148.0",
    "axios": "^1.6.8",
    "btoa": "1.2.1",
    "canvas": "2.10.2",
    "dockerode": "3.3.1",
    "express": "4.19.2",
    "fluent-ffmpeg": "2.1.2",
    "fluent-ffmpeg-multistream": "1.0.0",
    "kurento-client": "6.16.0",
    "livekit-server-sdk": "^1.2.7",
    "minio": "8.0.0",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^2.6.7",
    "node-localstorage": "2.2.1",
    "node-os-utils": "1.3.5",
    "openvidu-browser": "^2.23.0",
    "p-limit": "^3.1.0",
    "platform": "1.3.6",
    "proper-lockfile": "^4.1.2",
    "selenium-webdriver": "4.17.0",
    "stream-chunker": "1.2.8",
    "wrtc": "0.4.7",
    "ws": "^8.7.0",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.3",
    "@types/dockerode": "3.2.3",
    "@types/express": "4.17.12",
    "@types/multer": "^1.4.7",
    "@types/node": "14.14.31",
    "@types/node-cleanup": "^2.1.2",
    "@types/node-fetch": "2.6.1",
    "@types/node-os-utils": "1.2.0",
    "@types/platform": "1.3.3",
    "@types/proper-lockfile": "^4.1.4",
    "@types/selenium-webdriver": "4.1.13",
    "@types/ws": "7.4.1",
    "@types/yargs": "^17.0.10",
    "node-cleanup": "^2.1.2",
    "prettier": "2.2.0",
    "ts-node": "10.9.1",
    "typescript": "4.9.4"
  },
  "overrides": {
    "@types/mime": "3.0.4"
  }
}
